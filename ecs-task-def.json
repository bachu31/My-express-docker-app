{
    "family": "cst-Badri-Docker-app-Task",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "curl -f",
                    "http://localhost:3000/health || exit 1"
                ],
                "interval": 30,
                "retries": 3,
                "startPeriod": 20,
                "timeout": 5
            },
            "image": "<IMAGE_URI>",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/cst-Badri-Docker-app-Task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "eu-north-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "mountPoints": [],
            "name": "node-app",
            "portMappings": [
                {
                    "appProtocol": "http",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "name": "3000",
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::353524943445:role/ecsTaskDockerExecutionRole",
    "executionRoleArn": "arn:aws:iam::353524943445:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}